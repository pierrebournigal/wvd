{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {},
    "functions": [],
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2020-06-01",
            "name": "Hub",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/pierrebournigal/wvd/master/hub/hub.json",
                    "contentVersion": "1.0.0.0"
                },
                "parametersLink": {
                    "uri":"https://raw.githubusercontent.com/pierrebournigal/wvd/master/hub/hub.parameters.json",
                    "contentVersion":"1.0.0.0"
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2020-06-01",
            "name": "WVD",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/pierrebournigal/wvd/master/wvd/wvd.json",
                    "contentVersion": "1.0.0.0"
                },
                "subnet": {
                    "value": "[reference('Hub').outputs.resourceIDsubnet.value]"
                },
                "virtualNetworkId": {
                    "value": "[reference('Hub').outputs.virtualNetworkID.value]"
                }
            },
            "dependsOn": [
                "Hub"
            ]
        }
    ],
    "outputs": {}
}