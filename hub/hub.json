{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {},
    "functions": [],
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2020-06-01",
            "name": "VNet",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/pierrebournigal/wvd/master/hub/nestedtemplates/vnet.json",
                    "contentVersion": "1.0.0.0"
                },
                "parametersLink": {
                    "uri":"https://raw.githubusercontent.com/pierrebournigal/wvd/master/hub/nestedtemplates/vnet.parameters.json",
                    "contentVersion":"1.0.0.0"
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2020-06-01",
            "name": "LogAnalytics",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/pierrebournigal/wvd/master/hub/nestedtemplates/loganalytics.json",
                    "contentVersion": "1.0.0.0"
                },
                "parametersLink": {
                    "uri":"https://raw.githubusercontent.com/pierrebournigal/wvd/master/hub/nestedtemplates/loganalytics.parameters.json",
                    "contentVersion":"1.0.0.0"
                }
            }
        }
    ],
    "outputs": {
        "resourceIDsubnet": {
        "type": "string",
        "value": "[reference('VNet').outputs.resourceIDsubnet.value]"
        },
        "virtualNetworkID": {
        "type": "string",
        "value": "[reference('VNet').outputs.virtualNetworkID.value]"
        }
    }
}