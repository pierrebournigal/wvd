{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {},
    "functions": [],
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2020-06-01",
            "name": "VNet",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/pierrebournigal/wvd/master/hub/nestedtemplates/vnet.json",
                    "contentVersion": "1.0.0.0"
                },
                "parametersLink": {
                    "uri":"https://raw.githubusercontent.com/pierrebournigal/wvd/master/hub/nestedtemplates/vnet.parameters.json",
                    "contentVersion":"1.0.0.0"
                }
            }
        },
        {
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2020-06-01",
        "name": "StorageAccount",
        "properties": {
                "mode": "Incremental",
                "templateLink": {
                "uri": "https://raw.githubusercontent.com/pierrebournigal/wvd/master/wvd/nestedtemplates/fslogix.json",
                "contentVersion":"1.0.0.0"
                },
                "parameters": 
                {
                    "subnet": {
                     "value": "[reference('VNet').outputs.resourceIDsubnet.value]"
                },
                     "virtualNetworkId": {
                     "value": "[reference('VNet').outputs.virtualNetworkID.value]"
                },
                    "location": {
                    "value": "francecentral"
                },
                    "storageAccountName": {
                    "value": "safslogixlambda"
                },
                    "accountType": {
                    "value": "Premium_LRS"
                },
                    "kind": {
                    "value": "FileStorage"
                },
                    "minimumTlsVersion": {
                    "value": "TLS1_2"
                },
                    "supportsHttpsTrafficOnly": {
                    "value": true
                },
                    "allowBlobPublicAccess": {
                    "value": false
                },
                    "networkAclsBypass": {
                    "value": "AzureServices"
                },
                    "networkAclsDefaultAction": {
                    "value": "Allow"
                },
                    "isShareSoftDeleteEnabled": {
                    "value": true
                },
                    "shareSoftDeleteRetentionDays": {
                    "value": 7
                },
                    "privateEndpointName": {
                    "value": "pvefslogix"
                },
                    "privateLinkResource": {
                    "value": "[resourceId('Microsoft.Storage/storageAccounts','safslogixlambda')]"
                },
                    "targetSubResource": {
                    "value": [
                        "file"
                    ]
                },
                    "fileShareName": {
                    "value": "fslogix"
                }
        },
         "dependsOn": [
            "VNet"
        ]
    }
    }
    ],
    "outputs": {}
}